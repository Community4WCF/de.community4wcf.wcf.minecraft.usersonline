<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com/XSD/options.xsd">
	<import>
		<categories>
			<category name="minecraft.usersonline">
				<options>module_minecraft_usersonline</options>
				<showorder>45</showorder>
			</category>
			<category name="minecraft.usersonline.general">
				<parent>minecraft.usersonline</parent>
				<showorder>1</showorder>
			</category>
			<category name="minecraft.usersonline.sql">
				<parent>minecraft.usersonline</parent>
				<showorder>2</showorder>
			</category>
		</categories>
		<options>
			<!-- module options -->
			<option name="module_minecraft_usersonline">
				<categoryname>module.content</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<!-- module options -->
			<option name="minecraft_usersonline_sql_tablename">
				<categoryname>minecraft.usersonline.sql</categoryname>
				<optiontype>text</optiontype>
				<defaultvalue>users_online</defaultvalue>
				<hidden>0</hidden>
				<showorder>1</showorder>
			</option>
			<option name="minecraft_usersonline_sql_feldname">
				<categoryname>minecraft.usersonline.sql</categoryname>
				<optiontype>text</optiontype>
				<defaultvalue>name</defaultvalue>
				<hidden>0</hidden>
				<showorder>2</showorder>
			</option>
			<option name="minecraft_usersonline_select_standaloneapplication">
				<categoryname>minecraft.usersonline.general</categoryname>
				<optiontype>standaloneapplicationselect</optiontype>
				<defaultvalue>27,48</defaultvalue>
				<showorder>1</showorder>
			</option>
			<option name="minecraft_usersonline_cachetime">
				<categoryname>minecraft.usersonline.general</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>300</defaultvalue>
				<showorder>2</showorder>
			</option>
			<option name="minecraft_usersonline_hidewithoutusersonline">
				<categoryname>minecraft.usersonline.general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>3</showorder>
			</option>
		</options>
	</import>
</data>